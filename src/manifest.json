{
    "manifest_version": 2,
    "name": "Phytonic",
    "version": "0.1",
    "author": "David Barrag√°n Merino (@bameda)",
    "description": "A funny Firefox addon to correct typos with the Phyton programming language name",
    "homepage_url": "https://github.com/bameda/firefox-addon-phytonic",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },
    "browser_action": {
        "default_icon": "icons/icon48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["libs/findAndReplaceDOMText.js", "content.js"]
        }
    ]
}
